#include<stdio.h>
int main()
{
	
int n, n1, resultado, suma;

	printf("Introduzca un numero:\n");
	scanf("%d",&n);
	printf("Introduzca otro numero:\n");
	scanf("%d",&n1);
	printf("Cuanto suman?\n");
	scanf("%d",&suma);
	resultado = n + n1;

	if (suma==resultado)
	{
		printf("Correcto\n");
	}
	
	else
	{
	printf("INCORRECTO: La suma es %d\n",resultado);;
	}
	
	return 0;
}
